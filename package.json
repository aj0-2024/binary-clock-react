{
  "version": "1.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "name": "binary-clock-react",
  "author": "Anirudh Janga",
  "module": "dist/binary-clock-react.esm.js",
  "size-limit": [
    {
      "path": "dist/binary-clock-react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/binary-clock-react.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "binary-clock-core": "^2.0.0",
    "clsx": "^1.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anirudh-janga/binary-clock-react"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.6.0",
    "@storybook/addon-essentials": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/react": "^6.0.26",
    "@types/jest": "^26.0.15",
    "@types/react": "^16.9.52",
    "@types/react-dom": "^16.9.8",
    "postcss": "^8.1.2",
    "rollup-plugin-postcss": "^3.1.8",
    "size-limit": "^4.6.0",
    "typescript": "^4.0.3"
  }
}
